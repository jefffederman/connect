<%= render "subnav" %>

<h1>Netsuite</h1>
<% if @netsuite_imports_presenter.imported.empty? %>
  <strong><%= t(".empty_successful_imports") %></strong>
<% else %>
  <strong><%= t(".imported_successfully") %></strong>
  <table>
    <thead>
      <tr>
        <th><%= t(".employee_title") %></th>
        <th><%= t(".internal_id_title") %></th>
        <th><%= t(".email_address_title") %></th>
        <th><%= t(".user_status_title") %></th>
      </tr>
    </thead>
    <tbody>
      <% @netsuite_imports_presenter.imported.each do |employee| %>
        <tr>
          <td>
            <%= employee.name %>
          </td>
          <td>
            <%= employee.internal_id %>
          </td>
          <td>
            <%= employee.email %>
          </td>
          <td>
            <%= employee.user_status %>
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>
